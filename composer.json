{
  "name": "njimx/nova-grid",
  "description": "Add coluns to create and Update resources",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "nibri10/nova-grid": "dev-master",
    "laravel/nova": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Nibri10\\NovaGrid\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "nibri10/nova-grid",
        "version": "master",
        "source": {
          "url": "https://github.com/nibri10/nova-grid.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Nibri10\\NovaGrid\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
